<mxfile host="app.diagrams.net">
  <diagram id="homecare" name="Healthcare at Home">
    <mxGraphModel dx="778" dy="507" grid="1" gridSize="10" guides="1" tooltips="1"
                  connect="1" arrows="1" fold="1" page="1" pageScale="1"
                  pageWidth="850" pageHeight="1100" math="0" shadow="0">

      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- Environment -->
        <mxCell id="env" value="Environment: HomeCare" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;" vertex="1" parent="1">
          <mxGeometry x="60" y="80" width="240" height="70" as="geometry"/>
        </mxCell>

        <!-- Objects -->
        <mxCell id="objects" value="Objects:&#xa;- Patient&#xa;- Room&#xa;- Device" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;" vertex="1" parent="1">
          <mxGeometry x="60" y="170" width="200" height="120" as="geometry"/>
        </mxCell>

        <!-- State -->
        <mxCell id="state" value="State Vars:&#xa;- location[p]&#xa;- heart_rate[p]&#xa;- has_consent[p]&#xa;- med_dispenser_locked[d]&#xa;- alert_active" 
                style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;" vertex="1" parent="1">
          <mxGeometry x="280" y="170" width="260" height="140" as="geometry"/>
        </mxCell>

        <!-- Agents -->
        <mxCell id="agents" value="Agents:&#xa;• HomeAssistant&#xa;• RemoteClinician" 
                style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;" vertex="1" parent="1">
          <mxGeometry x="560" y="80" width="260" height="90" as="geometry"/>
        </mxCell>

        <!-- Norms -->
        <mxCell id="norms" value="Norms:&#xa;- Privacy / Consent&#xa;- Escalation threshold&#xa;- Alert cooldown&#xa;- Device lock rules"
                style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673a6;" vertex="1" parent="1">
          <mxGeometry x="560" y="190" width="260" height="140" as="geometry"/>
        </mxCell>

        <!-- Arrows -->
        <mxCell id="arrow1" edge="1" parent="1" source="env" target="objects"
                style="endArrow=block;strokeColor=#6c8ebf;"/>
        <mxCell id="arrow2" edge="1" parent="1" source="env" target="state"
                style="endArrow=block;strokeColor=#6c8ebf;"/>
        <mxCell id="arrow3" edge="1" parent="1" source="agents" target="state"
                style="endArrow=block;strokeColor=#b85450;"/>
        <mxCell id="arrow4" edge="1" parent="1" source="agents" target="norms"
                style="endArrow=block;strokeColor=#b85450;"/>
        <mxCell id="arrow5" edge="1" parent="1" source="state" target="norms"
                style="endArrow=block;strokeColor=#82b366;"/>
      </root>

    </mxGraphModel>
